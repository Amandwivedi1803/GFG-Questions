class Solution{
    private:
    Node *middle(Node *head)
    {
        Node *slow=head,*fast=head;
        while(fast!=NULL && fast->next!=NULL)
        {
            slow=slow->next;
            fast=fast->next->next;
        }
        return slow;
    }
    public:
      
    int getMiddle(Node *head)
    {
        // Your code here
        Node *mid=middle(head);
        return mid->data;
    }
};

