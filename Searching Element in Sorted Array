 int searchInSorted(int arr[], int N, int K) 
    {   int ans=-1;
        for(int i=0;i<N;i++)
        {
            if(arr[i]==K)
            {   
                ans=1;
                break;
            }
            if(arr[i]>K)
            {
                break;
            }
        }
        return ans;
       // Your code here
       
    }
};
